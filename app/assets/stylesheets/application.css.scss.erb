.omb_btn-facebook {
  background: #3b5998;
  width:360px;
  margin:50px auto 30px;
  opacity:1;
  -webkit-transition: background  0.2s ease-in-out;
  -moz-transition:background  0.2s ease-in-out;
  -o-transition: background  0.2s ease-in-out;
  transition: background  0.2s ease-in-out;
}

.btn-my-page
{
  background:#F99D2B;
  width:360px;
  margin:17px 0 -7px 10px;
  opacity:1;
  -webkit-transition: background  0.2s ease-in-out;
  -moz-transition:background  0.2s ease-in-out;
  -o-transition: background  0.2s ease-in-out;
  transition: background  0.2s ease-in-out;
}

.photo-area .omb_btn-facebook
{
  position:absolute;
  top:269px;
  border-radius:0;
  width: 444px;
  background:#0E416D
}

.modal-body .omb_btn-facebook
{
  width: 365px;
  margin:20px auto 40px;
}

.navbar .omb_btn-facebook {
  width:205px;
  float:left;
  margin:8px 0 0 0;
  padding:6px 0 6px 10px !important;
}

.navbar .omb_btn-facebook span
{
  font-weight:400;
  display:block;
  position:relative;
  top:-1px;
}

.omb_btn-facebook:hover,
.navbar .omb_btn-facebook:hover,
