  background:#031C4F !important;
}

.btn-my-page:hover
{
  background:#BC6700
}
.btn-my-page i,
.omb_btn-facebook i
{
  float:left;
  font-size:44pt;
  color: #fff !important;
	opacity:0.9;
  margin-right:15px;
}

.navbar .omb_btn-facebook i
{
  font-size:14pt;
  margin:0;
  padding:0 10px 0 2px;
}

.btn-my-page strong,
.omb_btn-facebook strong
{
  display:block;
  text-align:left;
  font-size:20pt;
  color:#fff;
}

.content-about .btn{
  text-align:left;
}

.btn-my-page span,
.omb_btn-facebook span
{
  display:block;
  text-align:left;
  font-size:16pt;
  font-weight:300;
  margin:-2px 0 -6px;
  color:#fff;
}

.navbar .omb_btn-facebook span
{
  font-size:11pt;
  margin:-1px 0 0;
}

#parallaxByeBye,
#parallaxAbout{
  height:500px;
  background-color:#fff;
  background: url( <%= asset_path 'about.jpg' %> ) 50% 0 fixed;
  position: relative;
}

#parallaxByeBye{
  height:750px;
  background: url( <%= asset_path 'byebye.jpg' %> ) 50% 0 fixed;
}

#parallaxByeBye .parallax-text h2,
#parallaxAbout .parallax-text h2
{
  text-align:right;
  font-size:38pt;
  margin:150px 30px 0;
}

#parallaxByeBye .parallax-text h2
{
  margin-top:220px;
}

#parallaxAbout .parallax-text h2 strong
{
  display:block;
}

#parallaxBar{
  height:600px;
  background-color:#fff;
  background: url( <%= asset_path 'copa1440x900.jpg' %> ) 50% 0 fixed;
  position: relative;
}

#parallaxBarTwo{
  height:765px;
  background-color:#fff;
  background: url( <%= asset_path 'praia-fut.jpg' %> ) 50% 0 fixed;
  position: relative;
}

#parallaxBarThree{
  height:700px;
  background-color:#fff;
  background: url( <%= asset_path 'boite.jpg' %> ) 50% 0 fixed;
  position: relative;
}

.parallax-text
{
  position:relative;
  text-align:center;
}

#parallaxBarTwo .parallax-text,
#parallaxBarThree .parallax-text
{
  top:200px;
  max-width:720px;
  margin:0 auto;
}

#parallaxBarThree .parallax-text
{
  max-width:900px;
}

.parallax-text h3
{
  color:#fff;
  font-size: 32pt;
  text-shadow: $text-shadow;
  margin-bottom:20px;
}

.parallax-text h2
{
  color:#fff;
  font-size: 52pt;
  text-shadow: $text-shadow;
  margin-bottom:20px;
}

.parallax-text h1
{
  color:#fff;
  max-width:580px;
  line-height:24pt;
  margin:0 auto;
  font-size: 16pt;
  text-shadow: $text-shadow;
}

#parallaxBar .parallax-text h2,
#parallaxBarTwo .parallax-text h2,
#parallaxBarThree .parallax-text h2
{
  margin-top:100px;
  font-size:42pt;
}

#parallaxBarThree .parallax-text h2
{
  margin-top:70px;
}

.parallax-img
{
  display:table;
  margin:0 auto;
  width:525px;
}

.parallax-img img
{
  float:left;
  margin-right:15px;
  margin-top:15px;
  box-shadow: $text-shadow;
  border-radius: $border-radius;
}

